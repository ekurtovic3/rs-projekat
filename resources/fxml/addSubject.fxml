<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>
<AnchorPane prefHeight="131.0" prefWidth="241.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
    <stylesheets>
        <URL value="@/css/mainform.css" />
    </stylesheets>
   <children>
      <GridPane layoutX="22.0" layoutY="19.0" vgap="5.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="33.8" minHeight="10.0" prefHeight="33.8" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="32.4" minHeight="10.0" prefHeight="27.799999999999997" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Name" />
            <Label GridPane.rowIndex="1" />
            <TextField fx:id="fldNameSubject" GridPane.columnIndex="1" />
             <ButtonBar prefHeight="32.0" prefWidth="523.0" GridPane.columnSpan="2" GridPane.rowIndex="2">
                 <buttons>
                     <Button fx:id="btnCancelSubject" mnemonicParsing="false" onAction="#cancelSubject" style="-fx-background-color: lightpink; -fx-border-color: black; -fx-border-radius: 5px; -fx-background-radius: 5px;" text="Cancel" />
                     <Button fx:id="btnConfirmSubject" mnemonicParsing="false" onAction="#confirmSubject" style="-fx-border-color: black; -fx-background-color: greenyellow; -fx-background-radius: 5px; -fx-border-radius: 5px;" text="Confirm" />
                 </buttons>
             </ButtonBar>
         </children>

          
      </GridPane>
   </children>
</AnchorPane>
